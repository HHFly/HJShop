<?xml version="1.0" encoding="utf-8"?>
<com.mark.app.hjshop4a.widget.SwipeMenuLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"


    android:clickable="true">
    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">
        <View
           style="@style/BaseLine.Horizontal"
            android:layout_height="23px"
            android:id="@+id/view2"></View>

        <View
            style="@style/BaseLine.Vertical"
            android:layout_width="15px"
            android:layout_height="180px"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"></View>
        <View
            style="@style/BaseLine.Vertical"
            android:layout_width="15px"
            android:layout_height="180px"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:id="@+id/view3"></View>
    <ImageView
        android:id="@+id/item_sdv_invite_logo"
        android:layout_width="85px"
        android:layout_height="85px"
        android:layout_centerVertical="true"
        android:layout_marginBottom="20px"
        android:layout_marginLeft="45px"

      />

        <TextView
            android:id="@+id/item_sdv_invite_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/item_sdv_invite_logo"
            android:layout_marginStart="11px"
            android:layout_toEndOf="@+id/item_sdv_invite_logo"
            android:text="工商银行"
            android:textColor="@color/colorText"
            android:textSize="@dimen/textsize_28px" />

        <TextView
            android:id="@+id/item_sdv_invite_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toStartOf="@+id/view3"
           android:layout_marginTop="100px"
            android:text=""
            android:textColor="@color/colorText"
            android:textSize="@dimen/textsize_24px" />
    </com.zhy.autolayout.AutoRelativeLayout>
</com.mark.app.hjshop4a.widget.SwipeMenuLayout>