    <style name="BaseTab"></style>

    <style name="BaseTab.Layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="BaseTab.Layout.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="BaseTab.Layout.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="BaseTab.Iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@mipmap/ic_launcher</item>
    </style>

    <style name="BaseTab.Tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/normal_ceshi</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="BaseItem"></style>

    <style name="BaseRecommend">
        <item name="android:background">@drawable/layer_recommend_bg</item>
    </style>

    <style name="BaseItem.Layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BaseItem.Iv">
        <item name="android:src">@mipmap/ic_launcher</item>
    </style>

    <style name="BaseItem.Tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="BaseRefreshLayout">
        <item name="srlDragRate">0.7</item>
        <item name="srlHeaderMaxDragRate">2</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/background</item>
        <item name="srlEnableHeaderTranslationContent">true</item>

    </style>

    <style name="BaseRefreshLayout.RefreshLoadMore">
        <!--<item name="srlEnableLoadmoreWhenContentNotFull">true</item>-->
    </style>

    <style name="BaseRefreshLayout.UnLoadMore">
        <!--<item name="srlEnableLoadmore">false</item>-->
    </style>

    <style name="BaseRefreshLayout.UnRefresh">
        <!--<item name="srlEnableAutoLoadmore">true</item>-->
        <!--<item name="srlEnableLoadmoreWhenContentNotFull">true</item>-->
        <!--<item name="srlEnableRefresh">false</item>-->
    </style>


    <style name="BaseDialogAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_push_in</item>
        <item name="android:windowExitAnimation">@anim/dialog_push_out</item>
    </style>

    <style name="ListDialogAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_push_in_translate</item>
        <item name="android:windowExitAnimation">@anim/dialog_push_out_translate</item>
    </style>


    <style name="BaseImg">
        <item name="android:layout_width">88px</item>
        <item name="android:layout_height">88px</item>
    </style>

    <style name="BaseImg.DeleteFork">
        <item name="android:padding">26px</item>
        <item name="android:src">@mipmap/ic_delete_fork</item>
    </style>

    <style name="BaseImg.Right">
        <item name="android:layout_width">30px</item>
        <item name="android:layout_height">30px</item>
        <item name="android:src">@mipmap/ic_right_gray</item>
    </style>

    <style name="BaseImg.Right.White">
        <item name="android:layout_width">30px</item>
        <item name="android:layout_height">30px</item>
        <item name="android:src">@mipmap/ic_right_white</item>
    </style>

    <style name="BaseImg.Bottom">
        <item name="android:layout_width">30px</item>
        <item name="android:layout_height">30px</item>
        <item name="android:src">@mipmap/ic_bottom_gray</item>
    </style>

    <style name="BaseImg.Trash">
        <item name="android:layout_width">90px</item>
        <item name="android:padding">30px</item>
        <item name="android:layout_height">90px</item>
        <item name="android:src">@mipmap/ic_trash_gray</item>
    </style>

    <style name="BaseImg.Select">
        <item name="android:layout_width">34px</item>
        <item name="android:layout_height">34px</item>
        <item name="android:src">@drawable/select_create_order_select</item>
        <item name="layout_auto_basewidth">height</item>
    </style>

    <style name="BaseBanner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="banner_pointContainerBackground">@android:color/transparent</item>
        <item name="banner_indicatorGravity">bottom|center_horizontal</item>
        <item name="banner_pointDrawable">@drawable/select_home_tab_index</item>
        <item name="banner_placeholderDrawable">@mipmap/holder</item>
        <item name="banner_transitionEffect">accordion</item>
        <item name="banner_pointAutoPlayInterval">5000</item>
    </style>


    <style name="BaseBanner.UnAutoScroll">
        <item name="banner_pointAutoPlayAble">false</item>
    </style>

    <style name="BaseProgressBar"></style>

    <style name="BaseProgressBar.Loading">
        <item name="android:layout_width">150px</item>
        <item name="android:layout_height">150px</item>
        <item name="android:indeterminateDrawable">@drawable/pb_loading_before6</item>
        <item name="layout_auto_baseheight">width</item>
    </style>

    <style name="BaseProgressBar.Loading.Before6">
        <item name="android:indeterminateDrawable">@drawable/pb_loading_before6</item>
    </style>

    <style name="BaseProgressBar.Loading.After6">
        <item name="android:indeterminateDrawable">@drawable/pb_loading_after6</item>
    </style>
    <style name="Bill_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">26px</item>
        <item name="android:textColor">#333333</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="Bill_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">24px</item>
        <item name="android:textColor">#666666</item>

    </style>
</resources>