<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BaseLayout"></style>

    <style name="BaseLayout.AutoWidth" parent="BaseLayout.WrapMatch">
        <item name="layout_auto_basewidth">height</item>
    </style>

    <style name="BaseLayout.MatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="BaseLayout.WrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BaseLayout.MatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BaseLayout.WrapMatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="BaseBtn" parent="@style/Widget.AppCompat.Button.Borderless">
        <item name="android:text">@string/normal_ceshi</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="BaseBtn.Normal"></style>

    <style name="BaseBtn.Normal.Small">
        <item name="android:textSize">26px</item>
        <item name="android:background">@drawable/shape_btn_small_normal</item>
        <item name="android:layout_height">50px</item>
        <item name="android:minWidth">124px</item>
        <item name="android:paddingLeft">23px</item>
        <item name="android:paddingRight">23px</item>
        <item name="android:textColor">@color/btn_small_blue</item>
    </style>

    <style name="BaseBtn.Normal.Small.Fill">
        <item name="android:textSize">24px</item>
        <item name="android:background">@drawable/select_btn_normal_small_fill</item>
        <item name="android:layout_height">56px</item>
        <item name="android:minWidth">133px</item>
        <item name="android:paddingLeft">23px</item>
        <item name="android:paddingRight">23px</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="BaseBtn.Normal.Important">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80px</item>
        <!--<item name="android:layout_marginLeft">30px</item>-->
        <!--<item name="android:layout_marginRight">30px</item>-->
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">30px</item>
        <item name="android:background">@drawable/select_btn_normal_important</item>
    </style>


    <style name="BaseBtn.Normal.ImportantUnRadius">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80px</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">34px</item>
        <item name="android:background">@drawable/select_btn_normal_important_un_radius</item>
    </style>

    <style name="BaseBtn.Normal.Commonly">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">70px</item>
        <item name="android:textColor">@drawable/select_btn_normal_commonly_textcolor</item>
        <item name="android:textSize">28px</item>
        <item name="android:background">@drawable/select_btn_normal_commonly</item>
    </style>

    <style name="BaseLine"></style>

    <style name="BaseLine.Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/view_line_width</item>
        <item name="android:background">@color/view_line</item>
    </style>

    <style name="BaseLine.Vertical">
        <item name="android:layout_width">@dimen/view_line_width</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/view_line</item>
    </style>

    <style name="BaseSDV">
        <item name="actualImageScaleType">centerCrop</item>
        <item name="fadeDuration">150</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BaseSDV.AspectRatio">
        <item name="layout_auto_basewidth">height</item>
    </style>

    <style name="BaseSDV.MatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="BaseSDV.Circle">
        <item name="roundAsCircle">true</item>
        <item name="viewAspectRatio">1</item>
    </style>


